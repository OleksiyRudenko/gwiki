{
  "name": "gwiki",
  "version": "1.0.3",
  "description": "A simple application that turns a Google Drive folder into a wiki",
  "main": "index.html",
  "repository": "https://github.com/OleksiyRudenko/gwiki.git",
  "author": "Kael Shipman <kael.shipman@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "parcel ./src/index.html --public-url ./",
    "watch": "parcel watch ./src/index.html --public-url ./",
    "prebuild-dev": "node node.prebuild.js",
    "build-dev": "parcel build ./src/index.html --no-minify --public-url ./",
    "prebuild": "node node.prebuild.js",
    "build": "parcel build ./src/index.html --public-url ./",
    "push-gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup --verbose"
  },
  "devDependencies": {
    "fs-extra": "^5.0.0"
  }
}
